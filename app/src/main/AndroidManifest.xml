<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.animationsexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ViewAnimationActivity"/>
        <activity android:name=".ViewAnimationTranslationActivity"/>
        <activity android:name=".PropertyAnimationActivity"/>
        <activity android:name=".ViewPropertyAnimatorActivity"/>
        <activity android:name=".InterpolatorsActivity"/>
        <activity android:name=".LayoutTransitionActivity"/>
        <activity android:name=".RecyclerViewAnimationActivity"/>
        <activity android:name=".SimpleTransitionActivity"/>
        <activity android:name=".SceneTransitionActivity"/>
        <activity android:name=".AnimatedVectorDrawableActivity"/>
        <activity android:name=".PlayPauseActivity"/>
        <activity android:name=".ConstraintLayoutActivity"/>
    </application>

</manifest>
